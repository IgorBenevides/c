algoritmo "semnome"
var
   n ,v:inteiro
procedimento parouimpar(v:inteiro)
inicio
 se v%2=0 entao
    escreval (" Esse é um numero par ")
 senao
      escreval (" Esse é um numero impar")
 fimse
fimprocedimento

inicio
      escreva (" Digite um numero :")
      leia (n)
      parouimpar (n)
fimalgoritmo
