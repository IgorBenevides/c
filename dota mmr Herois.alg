algoritmo "semnome"
var
   heroi  :caracter
   mmr ,Sk ,Mars ,Cracudilo ,Balanar :inteiro
   
inicio
      escreva (" Qual heroi vc vai jogar ?")
      leia (heroi)
      se heroi = "sk" entao
         sk<-sk+mmr
      senao
           se heroi = "cracudilo" entao
              cracudilo <- craculilo + mmr
           senao
                se
           fimse

      fimse
fimalgoritmo
